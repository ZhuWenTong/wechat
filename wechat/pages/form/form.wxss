.int{
    width:100%;
    height:80rpx;
    border:1px solid #ccc;
    box-sizing: border-box;
    background: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}
.int input{
    height: 100%;
    width:90%;
}
.placehold{
    font-family: '微软雅黑';
    font-size: 16px;
}