.content{
    padding:10rpx;
    box-sizing: border-box;   
}
.int{
    width:100%;
    height:80rpx;
    padding-left: 10rpx;
    border:1px solid #ccc;
    box-sizing: border-box;
    background: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}
.int input{
    height: 100%;
    width:90%;
}
.placehold{
    font-family: '微软雅黑';
    font-size: 16px;
}



picker{
    height:60rpx;
    line-height: 60rpx;
}

.picker,
.radios,
.checkboxes{
    margin-top:20rpx;
    width:100%;
    background: #fff;
    padding-left: 10rpx;
    box-sizing: border-box;
    border: 1px solid #ccc;
    border-radius: 10rpx;
}

.radio-group,
checkbox-group{
    width:100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
}
.radio,
.checkbox{
    width:100%;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}

.btn{
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
}
.btn button{
    width:200rpx;
    height:80rpx;
    line-height: 80rpx;
}